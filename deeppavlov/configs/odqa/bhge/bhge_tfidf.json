{
  "chainer": {
    "in": [
      "question_raw"
    ],
    "out": [
      "rank_text_score_id"
    ],
    "pipe": [
      {
        "name": "tfidf_ranker",
        "top_n": 20,
        "active": true,
        "in": [
          "question_raw"
        ],
        "out": [
          "doc_ids",
          "doc_scores"
        ],
        "fit_on_batch": [
          "questions_raw"
        ],
        "vectorizer": {
          "name": "hashing_tfidf_vectorizer",
          "fit_on_batch": [
            "questions_raw"
          ],
          "save_path": "general_electrics/en_ge_book_tfidf.npz",
          "load_path": "general_electrics/en_ge_book_tfidf.npz",
          "tokenizer": {
            "name": "stream_spacy_tokenizer",
            "lemmas": true,
            "ngram_range": [
              1,
              2
            ],
            "stopwords": "sklearn"
          }
        }
      },
      {
        "name": "wiki_sqlite_vocab",
        "in": [
          "doc_ids"
        ],
        "out": [
          "context_raw",
          "doc_ids"
        ],
        "shuffle": false,
        "join_docs": false,
        "load_path": "general_electrics/ge_book.db"
      },
      {
        "name": "rank_doc_score_tuplifier",
        "in": ["context_raw", "doc_scores", "doc_ids"],
        "out": ["rank_text_score_id"]
      }
    ]
  },
  "train": {
    "validate_best": false,
    "test_best": false,
    "batch_size": 10000
  },
  "metadata": {
    "download": [
      "http://lnsigo.mipt.ru/export/deeppavlov_data/general_electrics.tar.gz"
    ]
  }
}
