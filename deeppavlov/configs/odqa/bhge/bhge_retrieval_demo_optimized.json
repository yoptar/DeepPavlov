{
  "chainer": {
    "in": [
      "question_raw"
    ],
    "out": [
      "rank_text_score_id_ensemble"
    ],
    "pipe": [
      {
        "name": "tfidf_ranker",
        "top_n": 20,
        "active": true,
        "in": [
          "question_raw"
        ],
        "out": [
          "doc_ids",
          "doc_scores"
        ],
        "fit_on_batch": [
          "questions_raw"
        ],
        "vectorizer": {
          "name": "hashing_tfidf_vectorizer",
          "fit_on_batch": [
            "questions_raw"
          ],
          "save_path": "general_electrics/en_ge_book_tfidf.npz",
          "load_path": "general_electrics/en_ge_book_tfidf.npz",
          "tokenizer": {
            "name": "stream_spacy_tokenizer",
            "lemmas": true,
            "ngram_range": [
              1,
              2
            ],
            "stopwords": "sklearn"
          }
        }
      },
      {
        "name": "wiki_sqlite_vocab",
        "in": [
          "doc_ids"
        ],
        "out": [
          "context_raw",
          "doc_ids"
        ],
        "shuffle": false,
        "join_docs": false,
        "load_path": "general_electrics/ge_book.db"
      },
      {
        "name": "wiki_sqlite_vocab",
        "load_path": "general_electrics/ge_book.db",
        "return_all_content": true,
        "shuffle": false,
        "join_docs": false,
        "in": [
          "question_raw"
        ],
        "out": [
          "context_raw_all",
          "doc_ids_all"
        ]
      },
      {
        "name": "query_paragraph_tuplifier",
        "in": [
          "question_raw",
          "context_raw_all",
          "doc_ids_all"
        ],
        "out": [
          "question_paragraph_tuples"
        ]
      },
      {
        "name": "tfhub_paragraph_ranker",
        "top_n": 20,
        "active": true,
        "ranker": {
          "name": "use_sentence_ranker",
          "active": false
        },
        "in": [
          "question_paragraph_tuples"
        ],
        "out": [
          "docs_use",
          "doc_scores_use",
          "doc_ids_use"
        ]
      },
      {
        "name": "squad_paragraph_ranker",
        "active": true,
        "top_n": 20,
        "ranker_config": "../deeppavlov/configs/squad/squad_selqa_noans.json",
        "type": "paragraph",
        "in": [
          "question_paragraph_tuples"
        ],
        "out": [
          "docs_squad",
          "doc_scores_squad",
          "doc_ids_squad"
        ]
      },
      {
        "name": "rank_doc_score_tuplifier",
        "id": "my_rank_doc_score_tuplifier",
        "in": [
          "docs_use",
          "doc_scores_use",
          "doc_ids_use"
        ],
        "out": [
          "rank_text_score_id_use"
        ]
      },
      {
        "ref": "my_rank_doc_score_tuplifier",
        "in": [
          "docs_squad",
          "doc_scores_squad",
          "doc_ids_squad"
        ],
        "out": [
          "rank_text_score_id_rnet"
        ]
      },
      {
        "ref": "my_rank_doc_score_tuplifier",
        "in": [
          "context_raw",
          "doc_scores",
          "doc_ids"
        ],
        "out": [
          "rank_text_score_id_tfidf"
        ]
      },
      {
        "name": "ensemble_ranker",
        "top_n": 5,
        "in": [
          "rank_text_score_id_tfidf",
          "rank_text_score_id_use",
          "rank_text_score_id_rnet"
        ],
        "out": [
          "rank_text_score_id_ensemble"
        ]
      }
    ]
  },
  "train": {
    "validate_best": false,
    "test_best": false,
    "batch_size": 10000
  },
  "metadata": {
    "download": [
     "http://lnsigo.mipt.ru/export/deeppavlov_data/general_electrics.tar.gz",
      "http://lnsigo.mipt.ru/export/deeppavlov_data/squad_selqa_model_noans_tf1.8.tar.gz",
      {
        "url": "http://lnsigo.mipt.ru/export/embeddings/wiki-news-300d-1M.vec",
        "subdir": "embeddings"
      },
      {
        "url": "http://lnsigo.mipt.ru/export/embeddings/wiki-news-300d-1M-char.vec",
        "subdir": "embeddings"
      }
    ]
  }
}